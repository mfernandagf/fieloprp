    <validationRules>
        <fullName>ValidateInvoiceStatus</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 NOT(TEXT(Status__c)=&apos;Pending&apos;),
 OR(
  ISCHANGED(Amount__c),
  ISCHANGED(Date__c),
  ISCHANGED(Description__c),
  ISCHANGED(Image__c),
  ISCHANGED(InvoiceNumber__c),
  ISCHANGED(Member__c)
 )
)</errorConditionFormula>
        <errorMessage>Only Invoices with status Draft are editable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateMember</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISNEW(),
 ISBLANK(  Member__c  )
)</errorConditionFormula>
        <errorMessage>Member is a required field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateMemberUpdate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISCHANGED(  Member__c ), 
NOT(ISBLANK(PRIORVALUE(Member__c ))), 
PRIORVALUE(Member__c )!=Member__c 
)</errorConditionFormula>
        <errorMessage>Can not update the member field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateStatusChange</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISCHANGED(Status__c),
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Draft&apos;, TEXT(Status__c)=&apos;Pending&apos;)),
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Draft&apos;, TEXT(Status__c)=&apos;Approved&apos;)),
  
  
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Pending&apos;, TEXT(Status__c)=&apos;Approved&apos;)),
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Pending&apos;, TEXT(Status__c)=&apos;Rejected&apos;)),
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Pending&apos;, TEXT(Status__c)=&apos;Draft&apos;)),
  
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Approved&apos;, TEXT(Status__c)=&apos;Canceled&apos;)),
  
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Rejected&apos;, TEXT(Status__c)=&apos;Draft&apos;)),
  
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Canceled&apos;, TEXT(Status__c)=&apos;Approved&apos;)),
  NOT(AND( TEXT(PRIORVALUE(Status__c))=&apos;Canceled&apos;, TEXT(Status__c)=&apos;Draft&apos;))
)</errorConditionFormula>
        <errorMessage>Status transition not allowed</errorMessage>
    </validationRules>
